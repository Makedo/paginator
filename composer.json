{
  "name": "makedo/php-paginator",
  "description": "Data source independent paginator for php",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleksandr Kryvous",
      "email": "krivousa@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Makedo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Makedo\\": "spec"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
